{
	"name": "n8n-nodes-veed",
	"version": "1.2.0",
	"description": "Generate AI talking videos from images and audio using Veed Fabric API. Create realistic talking videos, animation videos and AI avatars in n8n workflows.",
	"license": "MIT",
	"homepage": "https://github.com/veedstudio/n8n-nodes-veed",
	"keywords": [
		"n8n-community-node-package",
		"n8n",
		"veed",
		"fabric",
		"fal.ai",
		"video-generation",
		"ai",
		"talking-head",
		"lip-sync",
		"avatar",
		"ai-avatar",
		"talking-video",
		"animation"
	],
	"author": {
		"name": "Dmytro Baltak",
		"email": "dmytro.baltak@veed.io"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/veedstudio/n8n-nodes-veed.git"
	},
	"engines": {
		"node": ">=22.0.0"
	},
	"scripts": {
		"clean": "rimraf dist/",
		"build": "pnpm clean && n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"test": "cross-env NODE_ENV=test vitest run",
		"test:watch": "cross-env NODE_ENV=test vitest",
		"test:coverage": "cross-env NODE_ENV=test vitest run --coverage",
		"release": "n8n-node release",
		"prepublishOnly": "n8n-node prerelease"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/FalAiApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/Veed/Veed.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"@types/node": "^24.7.0",
		"@vitest/coverage-v8": "^3.2.4",
		"@vitest/ui": "^3.2.4",
		"cross-env": "^10.1.0",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"rimraf": "^6.0.1",
		"tsx": "^4.20.6",
		"typescript": "5.9.2",
		"vitest": "^3.2.4"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
